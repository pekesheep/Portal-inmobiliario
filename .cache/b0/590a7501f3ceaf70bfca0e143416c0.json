ewModel;\r\n};\r\n\r\nconst getRoomWord = rooms => {\r\n    return rooms > 1 ? 'habitaciones' : 'habitación';\r\n};\r\n\r\nconst getBathroomWord = bathrooms => {\r\n    return bathrooms > 1 ? 'baños' : 'baño';\r\n};"},"lineCount":null}},"error":null,"hash":"21a377400b7f061b86b52fa99689c62c","cacheData":{"env":{}}}